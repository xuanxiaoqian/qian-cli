    "@types/inquirer": "^9.0.1",
    "chalk": "^4.1.0",
    "clear-console": "^1.1.0",
    "commander": "^9.4.0",
    "ejs": "^3.1.8",
    "esbuild": "^0.15.6",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "inquirer": "^8.1.0",
    "ora": "^5.1.2",
    "prettier": "^2.7.1",
    "shelljs": "^0.8.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2",
    "vitepress": "^1.0.0-alpha.13",
    "zx": "^7.0.8"
