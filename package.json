{
  "name": "qian-cli",
  "version": "1.3.19",
  "description": "快速搭建vue3开发环境的脚手架",
  "main": "index.js",
  "bin": {
    "qian-cli": "./lib/index.js"
  },
  "scripts": {
    "dev:init": "ts-node-dev --respawn --transpile-only src/index.ts init xuan",
