{
  "name": "qian-cli",
  "version": "1.3.50",
  "description": "快速搭建vue3开发环境的脚手架",
  "main": "index.js",
  "bin": {
    "qian-cli": "./lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev:init": "ts-node-dev --respawn --transpile-only src/index.ts init xuan",
    "dev:create": "ts-node-dev --respawn --transpile-only src/index.ts create qwe",
    "dev:docs": "vitepress dev docs",
    "build:cli": "node ./scripts/build.js",
    "build:docs": "vitepress build docs",
    "serve:docs": "vitepress serve docs",
    "prepublishOnly": "zx ./scripts/prepublish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuanxiaoqian/qian-cli"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "keywords": [
    "qian-cli",
    "cli",
    "xuanxiaoqian"
  ],
  "author": "xuanxiaoqian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xuanxiaoqian/qian-cli/issues"
  },
  "homepage": "https://qian-cli.xuanxiaoqian.com/",
  "dependencies": {
    "clear-console": "^1.1.0",
    "commander": "^9.4.0",
    "ejs": "^3.1.8",
    "inquirer": "^8.1.0",
    "kolorist": "^1.6.0",
    "ora": "^5.1.2",
    "shelljs": "^0.8.5",
    "update-notifier": "^5.0.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/inquirer": "^9.0.1",
    "esbuild": "^0.15.6",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2",
    "vitepress": "^1.0.0-alpha.13",
    "zx": "^7.0.8",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3"
  }
}
